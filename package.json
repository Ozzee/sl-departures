{
  "name": "sldepartures",
  "version": "1.0.0",
  "description": "Web app for displaying the next departures of a stop in the Stockholm region public transit system.",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p && rm -rf dist/images && mkdir dist/images && cp img/* dist/images/",
    "start": "DEV=true webpack-dev-server",
    "deploy-client": "npm run build && aws s3 sync dist/ s3://$BUCKET",
    "deply-server": "cd serverless && serverless deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.10.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "bucker": "^1.1.1",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  }
}
